{
    "name": "rokzabukovec/commandhub",
    "type": "project",
    "description": "The hub for all terminal commands",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "haydenpierce/class-finder": "^0.4.4",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^1.0",
        "laravel/framework": "^v11.0",
        "laravel/jetstream": "^5.0",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^v10.8.3",
        "laravel/socialite": "^5.12",
        "laravel/tinker": "^v2.9",
        "mailersend/laravel-driver": "^2.5",
        "meilisearch/meilisearch-php": "^v1.7.0",
        "predis/predis": "^v2.0.3",
        "ramsey/uuid-doctrine": "^2.0",
        "spatie/laravel-permission": "^6.4.0",
        "spatie/laravel-tags": "^4.6.1",
        "tightenco/ziggy": "^v2.0.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.6",
        "spatie/laravel-ignition": "2.4.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
